#include <stdio.h>

void rotateLeft(int arr[], int n, int d) {
    int temp[d];  // store first d elements
    int i;

    // Step 1: Copy first d elements into temp
    for(i = 0; i < d; i++) {
        temp[i] = arr[i];
    }

    // Step 2: Shift the rest of the array to the left
    for(i = d; i < n; i++) {
        arr[i - d] = arr[i];
    }

    // Step 3: Put temp elements at the end
    for(i = 0; i < d; i++) {
        arr[n - d + i] = temp[i];
    }
}

int main() {
    int n, d;
    scanf("%d %d", &n, &d);  // input size and rotations

    int arr[n];
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);  // input array elements
    }

    rotateLeft(arr, n, d);

    // Print rotated array
    for(int i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
}
